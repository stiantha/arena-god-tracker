{
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600, s-maxage=86400"
          }
        ]
      },
      {
        "source": "/(.*)\\.(js|css|jpg|jpeg|png|gif|ico|svg|webp)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }
  